<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.bootjava.palette.reply.mapper.ReplyMapper">
	<select id="replySelectList" resultType="co.bootjava.palette.reply.vo.ReplyVO">
		select * from reply
	</select>
	<insert id="replyInsert" parameterType="co.bootjava.palette.reply.vo.ReplyVO">
		insert into reply values(REPLY_NUMBER.nextval,'${replyGiveId}','${replyContent}',sysdate)
	</insert>
</mapper>